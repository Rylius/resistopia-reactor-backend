{
    "server": {
        "port": 8081
    },
    "websockets": {
        "compression": false
    },
    "log4js": {
        "appenders": {
            "file": {
                "type": "file",
                "filename": "logs/reactor_backend.log",
                "maxLogSize": 10485760,
                "numBackups": 3,
                "compress": false,
                "encoding": "utf-8"
            },
            "console": {
                "type": "console"
            }
        },
        "categories": {
            "default": {
                "appenders": [
                    "console",
                    "file"
                ],
                "level": "trace"
            }
        }
    }
}
